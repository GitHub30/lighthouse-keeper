{
  "version": "0.0.2",
  "description": "web.dev ligthhosue",
  "author": "Eric Bidelman <ebidel@>",
  "main": "index.html",
  "license": "Apache-2.0",
  "scripts": {
    "watch:js": "rollup -c --watch",
    "watch:css": "node-sass --watch ./public/app.scss ./public/app.min.css",
    "build:css": "node-sass ./public/app.scss ./public/app.min.css -t compressed",
    "build:js": "rollup -c",
    "build": "npm run build:css && npm run build:js",
    "prestart-disabled": "npm run build",
    "start": "node --experimental-modules server.mjs",
    "deploy": "gcloud app deploy app.yaml --project lighthouse-ci"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "lit-html": "^0.11.4",
    "node-sass": "^4.9.3",
    "rollup": "^0.66.4",
    "rollup-plugin-terser": "^3.0.0",
    "lighthouse": "^3.2.1",
    "express": "^4.16.3",
    "@firebase/firestore": "^0.8.2",
    "rollup-plugin-includepaths": "^0.2.3"
  },
  "devDependencies": {
    "lighthouse": "^3.2.1",
    "rollup-plugin-terser": "^3.0.0"
  }
}